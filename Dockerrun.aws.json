{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "server",
      "image": "xrchitron/itineraryx-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "xrchitron/itineraryx-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "xrchitron/itineraryx-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "memory": 128
    }
  ]
}
